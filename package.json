{
  "name": "ssocket-js",
  "version": "1.0.26",
  "description": "仿 Koa 中间件控制的 WebSocket 服务的 Web 客户端程序",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "tsc": "rm -fr ./dist & tsc --outDir ./dist",
    "types": "rm -fr ./types & tsc -d --emitDeclarationOnly --allowJs false --declarationDir ./types",
    "build": "webpack",
    "deldts":"rm -fr ./src/*.d.ts"
  },
  "keywords": [
    "socket",
    "websocket",
    "koa",
    "socket-js",
    "websocket-js"
  ],
  "author": "summer.li",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.2",
    "pako": "^2.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/pako": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "ts-loader": "^8.0.14",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
}
